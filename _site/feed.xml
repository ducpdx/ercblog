	function render(el) {
		  'use strict';

		  var isBlock = window.getComputedStyle(el).display === 'block';
		  var latex = el.textContent;

		  el.setAttribute('data-maths', latex);

		  try {
			el.classList.add('katex-rendered');
			window.katex.render(latex, el, { displayMode: isBlock });
		  } catch(e) {
